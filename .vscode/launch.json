  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/boot.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "BACKENDS": "192.168.122.220,192.168.122.221,192.168.122.222",
        "SERVICES": "80:30080",
        "HAPROXY_CONFIG_PATH":"/tmp/haproxy.cfg"
      }
    }
  ]
}
